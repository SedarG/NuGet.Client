{
  "version": "3.5.0-*",

  "description": "NuGet 3 restore for dotnet CLI",
  "buildOptions": {
    "emitEntryPoint": false,
    "warningsAsErrors": true,
    "xmlDoc": true,
    "nowarn": [
      "CS1591"
    ],
    "compile": {
      "include": [
        "../../../Shared/*.cs"
      ]
    }
  },

  "dependencies": {
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": "portable-net45+win8",
      "buildOptions": {
        "define": [
          "IS_CORECLR"
        ]
      },
      "dependencies": {
        "Microsoft.Build.Framework": "0.1.0-preview-00024-160610",
        "Microsoft.Build.Utilities.Core": "0.1.0-preview-00024-160610",
        "NuGet.Commands": {
          "target": "project"
        },
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      }
    }
  }
}
